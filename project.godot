; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Lord of The World"
config/version="1.0-debug"
config/tags=PackedStringArray("lord_of_the_world")
run/main_scene="res://src/scenes/loading-scene/enter_loading.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/bg_color=Color(0.187176, 0.187176, 0.187176, 1)
boot_splash/show_image=false
config/icon="res://src/ui/characters/c2.png"

[autoload]

GLOBAL="*res://src/scripts/global-singleton/global_variables.gd"

[display]

window/size/viewport_width=500
window/size/viewport_height=900
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=5
window/vsync/vsync_mode=0

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://android/": "gray",
"res://src/": "green",
"res://src/fonts/": "orange",
"res://src/scenes/": "green",
"res://src/scripts/": "green",
"res://src/ui/": "purple"
}

[gui]

theme/custom_font="res://src/fonts/inika/Inika-Bold.ttf"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
renderer/rendering_method.web="mobile"
textures/vram_compression/import_etc2_astc=true
