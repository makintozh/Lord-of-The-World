; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Lord of The World"
config/version="1"
config/tags=PackedStringArray("develop", "lord_of_the_world", "stable")
run/main_scene="res://src/scenes/auth-scenes/login_ui.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 0)

[autoload]

GLOBAL="*res://src/scripts/singleton/global_variables.gd"
CONFIG="*res://src/scripts/singleton/config.gd"
SceneManager="*res://src/scripts/singleton/SceneManager.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
Shaker="*res://addons/shaker/src/Shaker.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=500
window/size/viewport_height=975
window/size/always_on_top=true
window/size/extend_to_title=true
window/size/window_height_override=1050
window/energy_saving/keep_screen_on=false
window/stretch/mode="viewport"
window/stretch/aspect="keep_width"
window/per_pixel_transparency/allowed=true
window/handheld/orientation=1
window/vsync/vsync_mode=0
window/size/window_height_override.windows=975

[editor]

movie_writer/mjpeg_quality=1.0
movie_writer/disable_vsync=true
movie_writer/fps=300
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/CodeEditorBackground/plugin.cfg", "res://addons/awaitable_http_request/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/dialogue_nodes/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/shaker/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://android/": "gray",
"res://build-keys/": "gray",
"res://src/": "green",
"res://src/fonts/": "orange",
"res://src/scenes/": "green",
"res://src/scripts/": "green",
"res://src/ui/": "purple"
}

[gui]

theme/custom_font="res://src/fonts/ofont.ru_Hermes.ttf"

[gui_transitions]

config/default/auto_start=true
config/default/fade_layout=true
config/default/center_pivot=true
config/default/duration=1.0
config/default/delay=0.0
config/default/ease_type="IN_OUT"
config/default/animation_enter=4
config/default/animation_leave=4
config/default/transition_type="BACK"

[importer_defaults]

2d_array_texture={
"compress/mode": 0
}
scene={
"animation/fps": 120.0,
"meshes/force_disable_compression": true
}
oggvorbisstr={
"loop": true
}
texture={
"compress/channel_pack": 1,
"detect_3d/compress_to": 0,
"editor/convert_colors_with_editor_theme": true,
"editor/scale_with_editor_scale": true,
"mipmaps/generate": true,
"process/hdr_as_srgb": true,
"process/hdr_clamp_exposure": true,
"process/premult_alpha": true
}
font_data_bmfont={
"compress": false
}
font_data_dynamic={
"compress": false,
"generate_mipmaps": true,
"msdf_pixel_range": 100,
"msdf_size": 250,
"multichannel_signed_distance_field": true
}
font_data_image={
"compress": false
}
wavefront_obj={
"force_disable_mesh_compression": true
}
csv_translation={
"compress": false
}
animation_library={
"animation/fps": 120.0
}
cubemap_texture={
"compress/mode": 0
}
cubemap_array_texture={
"compress/mode": 0
}
3d_texture={
"compress/mode": 0
}

[input]

ENTER={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194309,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

textures/canvas_textures/default_texture_filter=2
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
textures/lossless_compression/force_png=true
textures/default_filters/use_nearest_mipmap_filter=true
environment/defaults/default_clear_color=Color(0.255659, 0.255659, 0.255659, 1)
anti_aliasing/quality/msaa_2d=2
viewport/hdr_2d=true
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_debanding=true
